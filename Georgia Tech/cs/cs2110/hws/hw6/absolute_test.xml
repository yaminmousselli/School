<?xml version="1.0"?>
<test-suite>

    <test-case>
        <name>absolute(1)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>10000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-value><address>U</address><value>1</value></test-value>
            <test-value><address>ANSWER</address><value>50</value></test-value>
        </input>
        <output>
            <test-value condition="equals" points="5">
                <address>ANSWER</address><value>1</value>
            </test-value>
        </output>
    </test-case>

